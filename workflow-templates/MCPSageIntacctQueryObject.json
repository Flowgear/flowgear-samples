                        "Value": "{\r\n  \"object\": \"accounts-receivable/customer\",\r\n  \"fields\": [\r\n    \"name\",\r\n    \"status\"\r\n  ],\r\n  \"filters\": [\r\n    {\r\n      \"$eq\": {\r\n        \"status\": \"active\"\r\n      }\r\n    }\r\n  ],\r\n  \"orderBy\": [\r\n    {\r\n      \"name\": \"desc\"\r\n    }\r\n  ]\r\n}"
