            {
                "NodeId": "0199879a-6bc4-87a8-405b-3f46d064300c",
                "Name": "Script",
                "ShortDescription": "Validate Select Query",
                "LongDescription": "We validate that it is a 'Select' query. We do not want the LLM to be able to invoke any query.",
                "Version": "2.1.0.16",
                "Properties": [
                    {
                        "FlowDirection": 1,
                        "SubProperties": [
                            {
                                "Name": "AdditionalAssemblyPaths",
                                "TypeName": "System.String",
                                "ExtendedType": 4
                            }
                        ],
                        "Behaviour": 0,
                        "CustomPropertyEscaping": 0,
                        "Schema": "(None)",
                        "IsSecret": false,
                        "Name": "Connection",
                        "TypeName": "Flowgear.Nodes.Script.Connection",
                        "ExtendedType": 2
                    },
                    {
                        "FlowDirection": 1,
                        "Behaviour": 0,
                        "CustomPropertyEscaping": 0,
                        "Schema": "",
                        "IsSecret": false,
                        "Name": "Script",
                        "TypeName": "System.String",
                        "ExtendedType": 4,
                        "Value": "using System;\r\nusing System.IO;\r\nusing System.Collections.Generic;\r\nusing System.Xml;\r\nusing System.Linq;\r\nusing System.Xml.Linq;\r\nusing Newtonsoft.Json;\r\nusing System.Text.RegularExpressions;\r\n\r\nnamespace CSharpScript\r\n{\r\n    public partial class Processor\r\n    {  \r\n        public object Process()\r\n        {\r\n            if (!IsSelectQuery(query.ToString()))\r\n            {\r\n                throw new Exception(\"The provided query is not valid.\");\r\n            }\r\n          \r\n            return null;\r\n        }\r\n\r\n        public bool IsSelectQuery(string query)\r\n        {\r\n            if (string.IsNullOrWhiteSpace(query))\r\n                return false;\r\n  \r\n            // Normalize whitespace and case\r\n            string trimmed = query.Trim().ToUpperInvariant();\r\n  \r\n            // Quick allowlist: must start with SELECT\r\n            if (!trimmed.StartsWith(\"SELECT\"))\r\n                return false;\r\n  \r\n            // Block some obvious dangerous keywords\r\n            string[] disallowed = { \"DELETE\", \"UPDATE\", \"INSERT\", \"DROP\", \"ALTER\", \"EXEC\", \"TRUNCATE\" };\r\n            foreach (var word in disallowed)\r\n            {\r\n                if (Regex.IsMatch(trimmed, string.Format(@\"\\b{0}\\b\", word), RegexOptions.IgnoreCase))\r\n                    return false;\r\n            }\r\n  \r\n            // Basic pattern check (SELECT ... FROM ...)\r\n            return Regex.IsMatch(trimmed, @\"^SELECT\\s+.+\\s+FROM\\s+\", RegexOptions.IgnoreCase);\r\n        }\r\n    }\r\n}"
                    },
                    {
                        "FlowDirection": 2,
                        "Behaviour": 0,
                        "CustomPropertyEscaping": 0,
                        "IsSecret": false,
                        "Name": "Value",
                        "TypeName": "System.Object",
                        "ExtendedType": 4
                    },
                    {
                        "FlowDirection": 1,
                        "Behaviour": 2,
                        "CustomPropertyEscaping": 0,
                        "IsSecret": false,
                        "Name": "query",
                        "TypeName": "System.String",
                        "ExtendedType": 0
                    }
                ],
